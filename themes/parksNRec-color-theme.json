{
	"name": "ParksNRec",
	"type": "dark",
	"colors": {
		"foreground": "#ffffff", 
		"focusBorder": "#0e0b0e",
		"descriptionForeground":"#12aa52",
		"errorForeground": "#aa1212",
		// centre area where you code
		"editor.background": "#111111",
		"editor.foreground": "#e6e6e6", 
		"editor.selectionHighlightBackground": "#12aa5185",
		"editor.lineHighlightBackground":"#101000",
		"editorCursor.background": "#111111",
		"editorCursor.foreground": "#12aa52",
		"editorLineNumber.activeForeground": "#12aa52",
		"editorLineNumber.foreground": "#808080",
		// Editor Groups & Tabs
		"editorGroup.border": "#12aa52",
		"tab.hoverBackground": "#161616",
		"tab.activeBorder": "#12aa52",
		// folder structure
		"sideBarTitle.foreground": "#B0B3B8",
		"sideBar.background": "#111111",
		"sideBar.foreground": "#ffffff",
		"sideBar.border": "#080808",
		"sideBarSectionHeader.background": "#121212",
		"sideBarSectionHeader.foreground": "#12aa52",
		// buttons from sidebar
		"button.background": "#12aa52",
		"button.foreground": "#ffffff",
		"button.hoverBackground": "#70cc97", 
		// dropdown in terminal
		"dropdown.background": "#0c0c0c",
		"dropdown.border": "#0c0c0c",
		"dropdown.foreground": "#B0B3B8",
		"dropdown.listBackground": "#0c0c0c",
		// bottom and right scrollbar
		"scrollbarSlider.background": "#12aa5175",
		"scrollbarSlider.hoverBackground": "#12aa51d0",
		"scrollbarSlider.activeBackground": "#12aa5175",
		// left most bar
		"activityBarBadge.background": "#12aa52",
		"activityBarBadge.foreground": "#e6e6e6",
		"activityBar.background": "#0c0c0c",
		"activityBar.foreground": "#e6e6e6",
		//
		"editorGroupHeader.tabsBackground": "#0c0c0c",
		"statusBar.background": "#100c08",
		"progressBar.background": "#12aa52",
		"menu.background": "#100c08",
		// 
		"titleBar.activeBackground":"#000000",
		"titleBar.border":"#080808",
		// leftside directory
		"list.activeSelectionBackground": "#0c0c0c",
		"list.activeSelectionForeground": "#12aa52",
		"list.inactiveSelectionBackground": "#0c0c0c",
		"list.inactiveSelectionForeground": "#12aa52",
		"list.hoverBackground": "#0c0c0c",
		"list.hoverForeground": "#12aa52",
		"list.errorForeground": "#e6e6e6",
		"list.warningForeground": "#e6e6e6",
		"list.invalidItemForeground": "#e6e6e6",
		"tab.inactiveBackground": "#0c0c0c",
		"tab.activeForeground": "#12aa52",
		"tab.inactiveForeground": "#e6e6e6",
		"menu.foreground": "#e6e6e6",
		"titleBar.activeForeground": "#e6e6e6",
		"titleBar.inactiveForeground":"#12aa52",
		"terminal.foreground": "#12aa52",
		"settings.textInputForeground": "#12aa52"
	},

///////////////////////////////////////////
// Happy with everything above this line //
///////////////////////////////////////////

"tokenColors": [
	{
		"name": "Comment",
		"scope": [
			"comment", 
			"string.quoted.docstring.multi.python", 
			"string.quoted.docstring.multi.python punctuation.definition.string.begin.python",
			"string.quoted.docstring.multi.python punctuation.definition.string.end.python"
		],
		"settings": {
			"foreground": "#80808081",
			"fontStyle": "italic"
		}
	},
	{
		"name": "String",
		"scope": [
			"string.quoted",
			"string.template",
			"punctuation.definition.string"
		],
		"settings": {
			"foreground": "#FFFFFF"
		}
	},
	{
		"name": "Punctuation within templates",
		"scope": "string.template meta.embedded.line",
		"settings": {
			"foreground": "#808080"
		}
	},
	{
		"name": "Variable",
		"scope": ["variable", "entity.name.variable"],
		"settings": {
			"foreground": "#12AA52"
		}
	},
	{
		"name": "Language variable",
		"scope": "variable.language",
		"settings": {
			"foreground": "#12AA52",
			"fontStyle": "bold"
		}
	},
	{
		"name": "Parameter",
		"scope": "variable.parameter",
		"settings": {
			"fontStyle": "italic"
		}
	},
	{
		"name": "Storage (declaration or modifier keyword)",
		"scope": ["storage.type", "storage.modifier"],
		"settings": {
			"foreground": "#808080"
		}
	},
	{
		"name": "Constant",
		"scope": "constant",
		"settings": {
			"foreground": "#FFFFFF"
		}
	},
	{
		"name": "Regex",
		"scope": "string.regexp",
		"settings": {
			"foreground": "#FFFFFF"
		}
	},
	{
		"name": "Number",
		"scope": "constant.numeric",
		"settings": {
			"foreground": "#FFFFFF"
		}
	},
	{
		"name": "Language constant (boolean, null)",
		"scope": "constant.language",
		"settings": {
			"foreground": "#FFFFFF"
		}
	},
	{
		"name": "Character escape",
		"scope": "constant.character.escape",
		"settings": {
			"foreground": "#FFFFFF"
		}
	},
	{
		"name": "Entity",
		"scope": "entity.name",
		"settings": {
			"foreground": "#5BC0DE"
		}
	},
	{
		"name": "HTML or XML tag",
		"scope": "entity.name.tag",
		"settings": {
			"foreground": "#a0ddb9"
		}
	},
	{
		"name": "HTML or XML tag brackets",
		"scope": ["punctuation.definition.tag"],
		"settings": {
			"foreground": "#a0ddb9"
		}
	},
	{
		"name": "Tag attribute",
		"scope": "entity.other.attribute-name",
		"settings": {
			"foreground": "#808080"
		}
	},
	{
		"name": "Tag attribute HTML",
		"scope": "entity.other.attribute-name.html",
		"settings": {
			"foreground": "#808080",
			"fontStyle": "italic"
		}
	},
	{
		"name": "Class",
		"scope": ["entity.name.type", "meta.attribute.class.html"],
		"settings": {
			"foreground": "#5BC0DE"
		}
	},
	{
		"name": "Inherited class",
		"scope": "entity.other.inherited-class",
		"settings": {
			"foreground": "#5BC0DE"
		}
	},
	{
		"name": "Function",
		"scope": ["entity.name.function", "variable.function"],
		"settings": {
			"foreground": "#AA5212"
		}
	},
	{
		"name": "JS Export",
		"scope": ["keyword.control.export.js", "keyword.control.import.js"],
		"settings": {
			"foreground": "#808080"
		}
	},
	{
		"name": "JS Numerics",
		"scope": ["constant.numeric.decimal.js"],
		"settings": {
			"foreground": "#808080"
		}
	},
	{
		"name": "Keyword",
		"scope": "keyword",
		"settings": {
			"foreground": "#808080"
		}
	},
	{
		"name": "Control keyword",
		"scope": "keyword.control",
		"settings": {
			"foreground": "#808080"
		}
	},
	{
		"name": "Operator",
		"scope": "keyword.operator",
		"settings": {
			"foreground": "#808080"
		}
	},
	{
		"name": "Special operator",
		"scope": [
			"keyword.operator.new",
			"keyword.operator.expression",
			"keyword.operator.logical"
		],
		"settings": {
			"foreground": "#808080"
		}
	},
	{
		"name": "Unit",
		"scope": "keyword.other.unit",
		"settings": {
			"foreground": "#FFFFFF"
		}
	},
	{
		"name": "Support",
		"scope": "support",
		"settings": {
			"foreground": "#5BC0DE"
		}
	},
	{
		"name": "Support function",
		"scope": "support.function",
		"settings": {
			"foreground": "#AA5212"
		}
	},
	{
		"name": "Support variable",
		"scope": "support.variable",
		"settings": {
			"foreground": "#12AA52"
		}
	},
	{
		"name": "Object literal key / property",
		"scope": ["meta.object-literal.key", "support.type.property-name"],
		"settings": {
			"foreground": "#12AA52"
		}
	},
	{
		"name": "Key-value separator",
		"scope": "punctuation.separator.key-value",
		"settings": {
			"foreground": "#808080"
		}
	},
	{
		"name": "Embedded punctuation",
		"scope": "punctuation.section.embedded",
		"settings": {
			"foreground": "#808080"
		}
	},
	{
		"name": "Template expression",
		"scope": [
			"punctuation.definition.template-expression.begin",
			"punctuation.definition.template-expression.end"
		],
		"settings": {
			"foreground": "#12AA52"
		}
	},
	{
		"name": "CSS property",
		"scope": [
			"support.type.property-name.css",
			"support.type.property-name.json"
		],
		"settings": {
			"foreground": "#12AA52"
		}
	},
	{
		"name": "JS Switch control",
		"scope": "switch-block.expr.js",
		"settings": {
			"foreground": "#7212AA52f1b8"
		}
	},
	{
		"name": "JS object path",
		"scope": "variable.other.constant.property.js, variable.other.property.js",
		"settings": {
			"foreground": "#AA5212"
		},
	},
	{
		"name": "Color",
		"scope": "constant.other.color",
		"settings": {
			"foreground": "#FFFFFF"
		}
	},
	{
		"name": "Font names",
		"scope": "support.constant.font-name",
		"settings": {
			"foreground": "#FFFFFF"
		}
	},
		
