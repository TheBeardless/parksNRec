{
	"name": "ParksNRec",
	"type": "dark",
	"colors": {
		"foreground": "#B0B3B8", // directory at top. 
		"focusBorder": "#0e0b0e",
		"descriptionForeground":"#2e6946",
		"errorForeground": "#eb34eb",
		// centre area where you code
		"editor.background": "#111111",
		"editor.foreground": "#aa6f21", 
		"editor.selectionHighlightBackground": "#12aa5185",
		"editor.lineHighlightBackground":"#101000",
		"editorCursor.background": "#eb34eb",
		"editorCursor.foreground": "#12aa52",
		"editorLineNumber.activeForeground": "#12aa52",
		"editorLineNumber.foreground": "#808080",
		// Editor Groups & Tabs
		"editorGroup.border": "#12aa52",
		"tab.hoverBackground": "#161616",
		"tab.activeBorder": "#12aa52",
		// folder structure
		"sideBarTitle.foreground": "#B0B3B8",
		"sideBar.background": "#0c0c0c",
		"sideBar.foreground": "#808080",
		"sideBar.border": "#080808",
		"sideBarSectionHeader.background": "#121212",
		"sideBarSectionHeader.foreground": "#12aa52",
		// buttons from sidebar
		"button.background": "#12aa52",
		"button.foreground": "#ffffff",
		"button.hoverBackground": "#70cc97",
		// dropdown in terminal
		"dropdown.background": "#0c0c0c",
		"dropdown.border": "#0c0c0c",
		"dropdown.foreground": "#B0B3B8",
		"dropdown.listBackground": "#0c0c0c",
		// rightmost bar
		"scrollbarSlider.background": "#0c0c0c",
		"scrollbarSlider.hoverBackground": "#12aa52",
		"scrollbarSlider.activeBackground": "#0c0c0c",
		// left most bar
		"activityBarBadge.background": "#12aa52",
		"activityBarBadge.foreground": "#0c0c0c",
		"activityBar.background": "#0c0c0c",
		"activityBar.foreground": "#12aa52",
		//
		"editorGroupHeader.tabsBackground": "#0c0c0c",
		"statusBar.background": "#100c08",
		"progressBar.background": "#12aa52",
		"menu.background": "#100c08",
		// 
		"titleBar.activeBackground":"#ec36e3",
		"titleBar.border":"#ec36e3",
		// leftside directory
		"list.activeSelectionBackground": "#0c0c0c",
		"list.activeSelectionForeground": "#12aa52",
		"list.inactiveSelectionBackground": "#0c0c0c",
		"list.inactiveSelectionForeground": "#12aa52",
		"list.hoverBackground": "#0c0c0c",
		"list.hoverForeground": "#12aa52",
		"list.errorForeground": "#ff7171",
		"list.warningForeground": "#ffc4a2",
		"list.invalidItemForeground": "#ffc4a2",
		"tab.inactiveBackground": "#0c0c0c",
		"tab.activeForeground": "#12aa52",
		"tab.inactiveForeground": "#B0B3B8",
		"menu.foreground": "#12aa52",
		"titleBar.activeForeground": "#12aa52",
		"titleBar.inactiveForeground":"#70cc97",
		"terminal.foreground": "#12aa52",
		"settings.textInputForeground": "#12aa52"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic underline",
				"foreground": "#ffffff57"
			}
		},
		{
      "name": "String",
      "scope": [
        "string.quoted",
        "string.template",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#e6e6e6"
      }
    },
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#12aa52",
				"fontStyle": "bold",
			}
		},
		{
      "name": "Parameter",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "italic"
      }
    },
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#d348c7"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#12aa52"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#12aa52"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#aa6f21"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#70cc97"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#e03ad3"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#aa6f21",
				"fontStyle": "italic"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#e03ad3"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.orther.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#aa6f21"
			}
		},
		{
			"name": "Entity Types",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#B2CCD6"
			}
		},
		{
			"name": "CSS Class and Support",
			"scope": [
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
				"source.less support.type.property-name",
				"source.stylus support.type.property-name",
				"source.postcss support.type.property-name"
			],
			"settings": {
				"foreground": "#e03ad3"
			}
		},
		{
			"name": "Sub-methods",
			"scope": [
				"entity.name.module.js",
				"variable.import.parameter.js",
				"variable.other.class.js"
			],
			"settings": {
				"foreground": "#e03ad3"
			}
		},
		{
			"name": "Language methods",
			"scope": [
				"variable.language"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#e03ad3"
			}
		},
		{
			"name": "entity.name.method.js",
