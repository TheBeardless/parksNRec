{
	"name": "ParksNRec",
	"type": "dark",
	"colors": {
		"foreground": "#ffffff", 
		"focusBorder": "#0e0b0e",
		"descriptionForeground":"#12aa52",
		"errorForeground": "#aa1212",
		// centre area where you code
		"editor.background": "#111111",
		"editor.foreground": "#e6e6e6", 
		"editor.selectionHighlightBackground": "#12aa5185",
		"editor.lineHighlightBackground":"#101000",
		"editorCursor.background": "#111111",
		"editorCursor.foreground": "#12aa52",
		"editorLineNumber.activeForeground": "#12aa52",
		"editorLineNumber.foreground": "#808080",
		// Editor Groups & Tabs
		"editorGroup.border": "#12aa52",
		"tab.hoverBackground": "#161616",
		"tab.activeBorder": "#12aa52",
		// folder structure
		"sideBarTitle.foreground": "#B0B3B8",
		"sideBar.background": "#111111",
		"sideBar.foreground": "#ffffff",
		"sideBar.border": "#080808",
		"sideBarSectionHeader.background": "#121212",
		"sideBarSectionHeader.foreground": "#12aa52",
		// buttons from sidebar
		"button.background": "#12aa52",
		"button.foreground": "#ffffff",
		"button.hoverBackground": "#70cc97", 
		// dropdown in terminal
		"dropdown.background": "#0c0c0c",
		"dropdown.border": "#0c0c0c",
		"dropdown.foreground": "#B0B3B8",
		"dropdown.listBackground": "#0c0c0c",
		// rightmost bar
		"scrollbarSlider.background": "#0c0c0c",
		"scrollbarSlider.hoverBackground": "#12aa52",
		"scrollbarSlider.activeBackground": "#0c0c0c",
		// left most bar
		"activityBarBadge.background": "#12aa52",
		"activityBarBadge.foreground": "#e6e6e6",
		"activityBar.background": "#0c0c0c",
		"activityBar.foreground": "#e6e6e6",
		//
		"editorGroupHeader.tabsBackground": "#0c0c0c",
		"statusBar.background": "#100c08",
		"progressBar.background": "#12aa52",
		"menu.background": "#100c08",
		// 
		"titleBar.activeBackground":"#000000",
		"titleBar.border":"#080808",
		// leftside directory
		"list.activeSelectionBackground": "#0c0c0c",
		"list.activeSelectionForeground": "#12aa52",
		"list.inactiveSelectionBackground": "#0c0c0c",
		"list.inactiveSelectionForeground": "#12aa52",
		"list.hoverBackground": "#0c0c0c",
		"list.hoverForeground": "#12aa52",
		"list.errorForeground": "#e6e6e6",
		"list.warningForeground": "#e6e6e6",
		"list.invalidItemForeground": "#e6e6e6",
		"tab.inactiveBackground": "#0c0c0c",
		"tab.activeForeground": "#12aa52",
		"tab.inactiveForeground": "#e6e6e6",
		"menu.foreground": "#e6e6e6",
		"titleBar.activeForeground": "#e6e6e6",
		"titleBar.inactiveForeground":"#12aa52",
		"terminal.foreground": "#12aa52",
		"settings.textInputForeground": "#12aa52"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#aa5112a1"
			}
		},
		{
      "name": "String",
      "scope": [
        "string.quoted",
        "string.template",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#e6e6e6"
      }
    },
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#12aa52",
				"fontStyle": "bold",
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable.language.this"			],
			"settings": {
				"foreground": "#0d7c3c",
				"fontStyle": "italic bold",
			}
		},
		{
      "name": "Parameter",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "italic",
				"foreground": "#d9534f"
      }
    },
		{
      "name": "Entity",
      "scope": "entity.name",
      "settings": {
        "foreground": "#12aa52"
      }
    },
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Test",
			"scope": [
				"meta.objectliteral"
			],
			"settings": {
				"foreground": "#12aa52"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#d9534f"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#939393"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"storage.type",

			],
			"settings": {
				"foreground": "#939393",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"storage.modifier"
			],
			"settings": {
				"foreground": "#939393"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#a0ddb9"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control.try",
				"keyword.control.catch",
				"keyword.control.flow",
				"storage.modifier.async",
			],
			"settings": {
				"foreground": "#aa5212"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.operator.comparison",
				"keyword.operator.assignment",
				"keyword.operator.logical",
				"storage.type.function.arrow"
			],
			"settings": {
				"foreground": "#12aa52"
			}
		},
		{
      "name": "JS Export",
      "scope": ["keyword.control.export.js", "keyword.control.import.js"],
      "settings": {
        "foreground": "#12aa52"
      }
    },
		{
      "name": "HTML or XML tag brackets",
      "scope": ["punctuation.definition.tag"],
      "settings": {
        "foreground": "#fff"
      }
    },
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#808080",
				"fontStyle": "italic bold"
			}
		},
		{
			"name": "TagTest",
			"scope": [
				"support.class.component"
			],
			"settings": {
				"foreground": "#12aa52",
				"fontStyle": "bold"
			}
		},
